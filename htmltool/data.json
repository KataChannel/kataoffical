const data = [ 
    { "ItemCode": "I100001", "DefaultPrice": "14000" },
    { "ItemCode": "I100002", "DefaultPrice": "24000" },
    { "ItemCode": "I100003", "DefaultPrice": "15000" }
];

let sqlCode = `UPDATE [TGG_DEV].[dbo].[M_Item] SET [DefaultPrice] = CASE `;

data.forEach(item => {
    sqlCode += `WHEN [ItemCode] = '${item.ItemCode}' THEN ${item.DefaultPrice} `;
});

sqlCode += `ELSE [DefaultPrice] END WHERE [ItemCode] IN (${data.map(item => `'${item.ItemCode}'`).join(", ")});`;

console.log(sqlCode);
