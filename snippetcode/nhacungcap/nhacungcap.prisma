model nhacungcap {
  id              String   @id @default(uuid())
  codeId          String   @unique
  title           String
  description     String?
  order           Int?     @default(1)
  createdBy       String? 
  createdByUser   User?   @relation(fields: [createdBy], references: [id])
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
